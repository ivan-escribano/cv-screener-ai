{
  "name": "cv-screener-backend",
  "version": "1.0.0",
  "description": "Backend for AI-Powered CV Screener",
  "type": "module",
  "scripts": {
    "dev": "tsx watch index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "generate:cvs": "tsx scripts/cv-generator/cv-generator.script.ts",
    "ingest": "curl -X POST http://localhost:3001/api/ingest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "rag",
    "cv-screener",
    "langraph"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/google": "^3.0.6",
    "@ai-sdk/openai": "^3.0.7",
    "@google/generative-ai": "^0.24.1",
    "@langchain/core": "^1.1.11",
    "@langchain/langgraph": "^1.0.7",
    "@langchain/openai": "^1.2.1",
    "@supabase/supabase-js": "^2.90.0",
    "@types/pdfkit": "^0.17.4",
    "ai": "^6.0.21",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "multer": "^2.0.2",
    "openai": "^6.15.0",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.17.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  }
}
